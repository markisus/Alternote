{% extends 'main.template' %}

{% block content %}
	<br/>
	<h4>Select a Course</h4> <hr/>
	<ul id="classes">
	{% for class in classes %}
		<li>
			<a href="[[ reverse_url('ViewCalendar', class._id) ]]">[[ class.name ]]</a>
			<p>[[ class.instructor ]]</p>
		</li>
	{% endfor %}
	</ul>
{% endblock content %}
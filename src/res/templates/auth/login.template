{% extends 'main.template' %}

{% block nav %}
	<li><a href="[[reverse_url('LoginHandler')]]">Login</a></li>
{% endblock %}

{% block content %}
	<br/>
	<h4>Log In</h4>
	<form method="post">
		[[ with_errors(form.email) ]]
		[[ with_errors(form.password) ]]
		[[ form.submit() ]]
	</form>
{% endblock content %}